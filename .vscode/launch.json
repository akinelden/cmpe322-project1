    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug PartA",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/part_a/part_a.out",
            "args": ["./blackbox", "part_a_out.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/part_a",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build part_a",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug PartB Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/part_b/box_call_server.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/part_b",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build part_b",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug PartB Client",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/part_b/part_b_client.out",
            "args": ["./blackbox", "./part_b_out.txt", "localhost"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/part_b",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}